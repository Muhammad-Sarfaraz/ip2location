{
    "manifest_version": 3,
    "name": "Ip2Location",
    "version": "1.0",
    "description": "Ip 2 Location: is a Chrome extension that offers precise geo-location data for any IP address",
    "permissions": [
        "contextMenus",
        "tabs"
    ],
    "icons": {
        "16": "icons/16x16.png",
        "48": "icons/48x48.png",
        "128": "icons/128x128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/16x16.png",
            "48": "icons/48x48.png",
            "128": "icons/128x128.png"
        }
    }
}